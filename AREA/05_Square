import java.util.Scanner;

public class _AREA {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        float side;
        System.out.println("Enter  the length of side of the Square :");
        side=s.nextFloat();
        System.out.println("Area of Square : "+side*side);
        s.close();
    }
}
